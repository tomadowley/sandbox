      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "importOrder": [
      "^react(.*)$",
      "^react-dom(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^molecule/(.*)$",
      "^organism/(.*)$",
      "^component/(.*)$",
      "^container/(.*)$",
      "^page/(.*)$",
      "^store/(.*)$",
      "^translation/(.*)$",
      "^utility/(.*)$",
      "^routerConfig(.*)$",
      "^const(.*)$",
      "^type(.*)",
      "^(.*)/(.*).(utils)(.*)$",
      "^[./]"
    ]
  }
